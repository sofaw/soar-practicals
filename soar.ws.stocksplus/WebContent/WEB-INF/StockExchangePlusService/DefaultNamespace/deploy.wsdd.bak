<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from StockExchangePlusService WSDL service -->

  <service name="StockExchangePlus" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://DefaultNamespace"/>
      <parameter name="wsdlServiceElement" value="StockExchangePlusService"/>
      <parameter name="schemaQualified" value="http://DefaultNamespace"/>
      <parameter name="wsdlServicePort" value="StockExchangePlus"/>
      <parameter name="className" value="DefaultNamespace.StockExchangePlusSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="StockExchangePlus"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getAveragePrice" qname="operNS:getAveragePrice" xmlns:operNS="http://DefaultNamespace" returnQName="retNS:getAveragePriceReturn" xmlns:retNS="http://DefaultNamespace" returnType="rtns:float" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:stock" xmlns:pns="http://DefaultNamespace" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getAveragePrice"/>

  </service>
</deployment>
